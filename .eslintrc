{
  {
    "extends": "standard"
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "comma-dangle": 1,
    "no-console": 1,
    "no-undef": 0,
    "no-unused-vars": 1,
    "no-var": 2,
    "prefer-const": 2,
    "quotes": [2, "single"],
    "strict": [2, "never"],
  }
}
